	<queryString language="SQL">
		<![CDATA[SELECT pp.idpatrimonio_producto as idpp,
to_char(pci.fecha_creado,'yyyy-MM-dd HH24:MI:00') as fecha,
pci.descripcion,
pu.nombre as ubicacion,
pci.referencia,pci.cantidad as cant,
pci.precio_compra as precio,
(pci.cantidad*pci.precio_compra) as subtotal
FROM patrimonio_carga_item pci,patrimonio_producto pp,patrimonio_ubicacion pu,patrimonio_carga pc
where  pci.fk_idpatrimonio_producto=pp.idpatrimonio_producto
and pp.fk_idpatrimonio_ubicacion=pu.idpatrimonio_ubicacion
and pci.fk_idpatrimonio_carga=pc.idpatrimonio_carga
and pc.estado='CARGADO'
order by pci.idpatrimonio_carga_item desc;]]>
