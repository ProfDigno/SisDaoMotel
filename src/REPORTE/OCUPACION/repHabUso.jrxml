	<queryString>
		<![CDATA[SELECT 
('HAB:'||h.nro_habitacion) as habitacion,
to_char(h.fec_ocupado_inicio,'yyyy-MM-dd') as fecha,
to_char(h.fec_ocupado_inicio,'HH24:MI:ss') as inicio,
to_char(h.fec_ocupado_fin,'HH24:MI:ss') as fin,
to_char((h.fec_ocupado_fin-h.fec_ocupado_inicio),'dd HH24:MI:ss') as tiempo,
v.monto_minimo as minimo,
v.monto_adicional as adicional,
v.monto_consumo as consumo,
(v.monto_minimo+v.monto_adicional+v.monto_consumo) as monto_total
FROM
  habitacion_recepcion h,venta v
  where h.idhabitacion_recepcion=v.fk_idhabitacion_recepcion
  and v.estado='TERMINADO'
  order by 1 desc,2 desc,3 desc limit 10]]>
	</queryString>
