	<queryString language="SQL">
		<![CDATA[select v.idrh_vale as idv,
(p.nombre||'-'||p.ruc) as persona,
case 
when date_part('month',v.fecha_creado)=1 then 'ENERO'
when date_part('month',v.fecha_creado)=2 then 'FEBRERO'
when date_part('month',v.fecha_creado)=3 then 'MARZO'
when date_part('month',v.fecha_creado)=4 then 'ABRIL'
when date_part('month',v.fecha_creado)=5 then 'MAYO'
when date_part('month',v.fecha_creado)=6 then 'JUNIO'
when date_part('month',v.fecha_creado)=7 then 'JULIO'
when date_part('month',v.fecha_creado)=8 then 'AGOSTO'
when date_part('month',v.fecha_creado)=9 then 'SEPTIEMBRE'
when date_part('month',v.fecha_creado)=10 then 'OCTUBRE'
when date_part('month',v.fecha_creado)=11 then 'NOVIEMBRE'
when date_part('month',v.fecha_creado)=12 then 'DICIEMBRE'
else 'error' end as mes,
to_char(v.fecha_creado,'yyyy-MM-dd') as fecha,
v.descripcion as descripcion,
v.estado as estado,
v.monto_vale as monto
from rh_vale v,persona p
where v.fk_idpersona=p.idpersona
and (v.estado='CERRADO' or v.estado='EMITIDO')
order by 2 desc,3 desc,v.fecha_creado desc]]>
	</queryString>
