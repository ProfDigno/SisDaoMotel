	<queryString>
		<![CDATA[select p.idproducto as idp,p.codigo_barra as codbarra,pc.nombre as categoria,p.nombre as producto,
p.precio_venta as pventa,
p.precio_compra as pcompra,
p.stock_actual as stock, 
coalesce((select sum(vi.cantidad) as cant from venta_item vi,venta v 
where vi.fk_idventa=v.idventa and vi.tipo_item='CARGADO' 
 and date(vi.fecha_creado)>='2023-06-01' and date(vi.fecha_creado)<='2023-06-25'  
and vi.fk_idproducto=p.idproducto),0) as c_venta, 
coalesce((select sum(vi.cantidad) as cant from venta_item_interno vi,venta_interno v 
where vi.fk_idventa_interno=v.idventa_interno and vi.tipo_item='EMITIDO' 
 and date(vi.fecha_creado)>='2023-06-01' and date(vi.fecha_creado)<='2023-06-25'  
and vi.fk_idproducto=p.idproducto),0) as c_interna,
coalesce((select sum(vi.cantidad) as cant from compra_item vi,compra v 
where vi.fk_idcompra=v.idcompra and vi.tipo_item='INGRESADO' 
 and date(vi.fecha_creado)>='2023-06-01' and date(vi.fecha_creado)<='2023-06-25'   
and vi.fk_idproducto=p.idproducto),0) as c_compra 
 FROM producto p,producto_categoria pc
where p.fk_idproducto_categoria=pc.idproducto_categoria
 and p.es_venta=true and p.es_compra=true 
order by 8 desc;]]>
	</queryString>
