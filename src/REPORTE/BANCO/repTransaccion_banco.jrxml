	<queryString language="SQL">
		<![CDATA[select tb.idtransaccion_banco as idtb,
(b.nombre||'-'||db.nro_cuenta) as banco,
date_part('month',tb.fecha_transaccion) as imes,
case 
when date_part('month',tb.fecha_transaccion)=1 then 'ENERO'
when date_part('month',tb.fecha_transaccion)=2 then 'FEBRERO'
when date_part('month',tb.fecha_transaccion)=3 then 'MARZO'
when date_part('month',tb.fecha_transaccion)=4 then 'ABRIL'
when date_part('month',tb.fecha_transaccion)=5 then 'MAYO'
when date_part('month',tb.fecha_transaccion)=6 then 'JUNIO'
when date_part('month',tb.fecha_transaccion)=7 then 'JULIO'
when date_part('month',tb.fecha_transaccion)=8 then 'AGOSTO'
when date_part('month',tb.fecha_transaccion)=9 then 'SEPTIEMBRE'
when date_part('month',tb.fecha_transaccion)=10 then 'OCTUBRE'
when date_part('month',tb.fecha_transaccion)=11 then 'NOVIEMBRE'
when date_part('month',tb.fecha_transaccion)=12 then 'DICIEMBRE'
else 'error' end as mes,
tb.fecha_transaccion as fecha,
tb.nro_transaccion as referencia,tb.concepto as concepto,
tb.monto_guarani as guarani,tb.monto_dolar as dolar
from transaccion_banco tb,dato_banco db,banco b
where tb.fk_iddato_banco=db.iddato_banco
and db.fk_idbanco=b.idbanco
order by 2 desc,3 desc,5 desc;]]>
