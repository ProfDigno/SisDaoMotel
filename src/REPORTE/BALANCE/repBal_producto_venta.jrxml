	<queryString language="SQL">
		<![CDATA[select  ('fecha') as fecha,
p.idproducto as idpro,pc.nombre as categoria,
vi.descripcion as descripcion,
vi.precio_venta as precio_v,
sum(vi.cantidad) as cant,
sum(vi.precio_venta*vi.cantidad) as ototal_venta,
sum(vi.precio_compra*vi.cantidad) as ototal_compra,
sum((vi.precio_venta*vi.cantidad)-(vi.precio_compra*vi.cantidad)) as oganancia
from caja_cierre cc,caja_cierre_item cci,caja_cierre_detalle ccd,venta v,venta_item vi,producto p,producto_categoria pc
where cc.idcaja_cierre=cci.fk_idcaja_cierre
and cci.fk_idcaja_cierre_detalle=ccd.idcaja_cierre_detalle
and ccd.fk_idventa=v.idventa
and v.idventa=vi.fk_idventa
and vi.fk_idproducto=p.idproducto
and p.fk_idproducto_categoria=pc.idproducto_categoria
and ccd.cerrado_por='DESOCUPADO'
and vi.cantidad>0
and date(cc.fecha_inicio)>=date('2023-07-01') and date(cc.fecha_inicio)<=date('2023-07-31') 
group by 1,2,3,4,5
order by 6 desc]]>
	</queryString>
